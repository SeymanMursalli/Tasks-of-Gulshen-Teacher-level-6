#include <iostream>
using namespace std;

int main() {
    int r, c;
    cin >> r >> c;

    int a[100][100];

    for (int i = 0; i < r; i=i+1)
        for (int j = 0; j < c; j=j+1)
            cin >> a[i][j];

    int c = 0, h = 1;
    int m = a[0][0], b = a[0][0];

    for (int i = 0; i < r; i=i+1) {
        for (int j = 0; j < c; j=j+1) {
            c=c+a[i][j];
            h=h*a[i][j];
            if (a[i][j] > m) m = a[i][j];
            if (a[i][j] < b) b = a[i][j];
        }
    }

    cout << "CÉ™mi: " << c << endl;
    cout << "Hasili: " << h << endl;
    cout << "Maxsimum: " << m << endl;
    cout << "Minminimum: " << b << endl;
}